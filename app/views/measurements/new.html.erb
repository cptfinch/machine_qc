<h1>New measurement</h1>

<%= error_messages_for :measurement %>

<% form_for(@measurement) do |f| %>
  <p>
    <b>Measurement type</b><br />
    <%= f.text_field :measurement_type_id %>
  </p>

  <p>
    <b>Qcsession</b><br />
    <%= f.text_field :qcsession_id %>
  </p>

  <p>
    <b>Value</b><br />
    <%= f.text_field :value %>
  </p>

  <p>
    <b>Machine</b><br />
    <%= f.text_field :machine_id %>
  </p>

  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', measurements_path %>
