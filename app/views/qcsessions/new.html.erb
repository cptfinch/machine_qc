<h1>New qcsession</h1>

<%= error_messages_for :qcsession %>

<% form_for(@qcsession) do |f| %>
  <p>
    Machine<br /> 
    <%= 
      collection_select(:qcsession, :machine_id, @machines, :id, :name, {:prompt => true})
    %>
  </p>

  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', qcsessions_path %>
